    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/vue": "8.1.0",
    "@types/jasmine": "5.1.12",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.20",
    "@types/node": "22.19.0",
    "@typescript-eslint/eslint-plugin": "8.46.2",
