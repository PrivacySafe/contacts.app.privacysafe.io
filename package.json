  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:deno": "bash ci/bundle-deno-code.sh",
    "build:gui": "vue-tsc --noEmit && vite build",
    "build": "pnpm lint && pnpm build:gui && pnpm build:deno",
