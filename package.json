{
  "name": "contacts.app.privacysafe.io",
  "version": "0.5.6",
  "private": true,
  "scripts": {
    "clean:coverage": "rimraf coverage",
    "dev": "vite",
    "build:deno": "deno bundle './src-deno/contacts-deno.ts' -- './app/contactDenoServices.js'",
    "build": "vue-tsc --noEmit && vite build && npm run build:deno",
    "serve": "vite preview",
    "stylelint": "stylelint --config stylelint.config.js 'src/**/*.{vue,scss}'",
    "stylelint:fix": "stylelint --config stylelint.config.js --fix 'src/**/*.{vue,scss}'",
    "linter": "eslint -c .eslintrc.js --ext .js,.vue,.ts, src",
    "linter:fix": "eslint -c .eslintrc.js --fix --ext .js,.vue,.ts, src",
    "lint": "yarn linter && yarn stylelint",
    "lint:fix": "yarn linter:fix && yarn stylelint:fix",
    "test": "yarn clean:coverage && jest",
    "test:watch": "yarn clean:coverage && jest --watch"
  },
  "dependencies": {
    "@v1nt1248/3nclient-lib": "^0.0.25",
    "lodash": "^4.17.21",
    "pinia": "^2.0.14",
    "vue": "^3.3.4",
    "vue-router": "4.2.4",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/vue": "^5.8.3",
    "@types/jasmine": "^3.9.1",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/devtools": "6.5.0",
    "@vuedx/typescript-plugin-vue": "^0.7.5",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^8.7.1",
    "jest": "^27.5.1",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "postcss-html": "^1.4.1",
    "sass": "^1.43.4",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-rscss": "^0.4.0",
    "stylelint-scss": "^4.2.0",
    "ts-jest": "^28.0.5",
    "typescript": "^5.0.4",
    "vite": "^2.9.13",
    "vue-jest": "^3.0.7",
    "vue-tsc": "^1.0.9"
  }
}
